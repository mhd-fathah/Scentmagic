    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script>

        const alertContainer = document.getElementById('alert-container');
        const alertType = alertContainer.getAttribute('data-type');
        const alertTitle = alertContainer.getAttribute('data-title');
        const alertMessage = alertContainer.getAttribute('data-message');
        const alertRedirect = alertContainer.getAttribute('data-redirect');

        if (alertType) {
            Swal.fire({
                icon: alertType,
                title: alertTitle,
                text: alertMessage,
            }).then(() => {
                if (alertRedirect) {
                    window.location.href = alertRedirect;
                }
            });
        }
    </script>

    <script>
        document.addEventListener("DOMContentLoaded", () => {

            const couponCode = document.getElementById("coupon-code");
            const couponStatus = document.getElementById("coupon-status");
            const couponDiscount = document.getElementById("coupon-discount");
            const couponValidFrom = document.getElementById("coupon-valid-from");
            const couponValidUntil = document.getElementById("coupon-valid-until");
            const couponUsageLimit = document.getElementById("coupon-usage-limit");
            const couponUsed = document.getElementById("coupon-used");
            const couponMinPurchase = document.getElementById("coupon-min-purchase");
            const editCouponBtn = document.getElementById("edit-coupon-btn");
            const deactivateForm = document.getElementById("deactivate-form");
            const deactivateCouponId = document.getElementById("deactivate-coupon-id");


            const viewDetailsButtons = document.querySelectorAll(".select-coupon");
            viewDetailsButtons.forEach((button) => {
                button.addEventListener("click", () => {

                    const couponRow = button.closest(".coupon-row");
                    const coupon = JSON.parse(couponRow.getAttribute("data-coupon"));


                    couponCode.textContent = coupon.code || "N/A";
                    couponStatus.textContent = coupon.status || "N/A";
                    couponDiscount.textContent =
                        coupon.type === "percentage"
                            ? coupon.discount + "%"
                            : "â‚¹" + coupon.discount;
                    couponValidFrom.textContent = new Date(coupon.validFrom).toLocaleDateString() || "N/A";
                    couponValidUntil.textContent = new Date(coupon.validUntil).toLocaleDateString() || "N/A";
                    couponUsageLimit.textContent = coupon.usageLimit || "N/A";
                    couponUsed.textContent = coupon.used || "N/A";
                    couponMinPurchase.textContent = coupon.minPurchase || "N/A";


                    editCouponBtn.classList.remove("hidden");
                    editCouponBtn.href = `/admin/coupon/edit/${coupon._id}`;
                    deactivateForm.classList.remove("hidden");
                    deactivateCouponId.value = coupon._id;
                });
            });
        });


    </script>
    <script>

        document.getElementById("statusFilter").addEventListener("change", function () {
            const selectedStatus = this.value;
            const rows = document.querySelectorAll(".coupon-row");

            rows.forEach(row => {
                const rowStatus = row.getAttribute("data-status");
                if (selectedStatus === "all" || rowStatus === selectedStatus) {
                    row.style.display = "";
                } else {
                    row.style.display = "none";
                }
            });
        });


        document.addEventListener('DOMContentLoaded', function () {

            document.querySelectorAll('.select-coupon').forEach(function (button) {
                button.addEventListener('click', function (e) {
                    const couponId = e.target.getAttribute('data-id');

                    if (!couponId) {
                        console.error('Coupon ID is missing!');
                        return;
                    }


                    fetch(`/admin/coupon/details/${couponId}`)
                        .then(response => {
                            if (!response.ok) {
                                throw new Error(`Failed to fetch coupon details: ${response.statusText}`);
                            }
                            return response.json();
                        })
                        .then(coupon => {

                            document.querySelector('#editCouponId').value = coupon._id || '';
                            document.querySelector('#editCode').value = coupon.code || '';
                            document.querySelector('#editType').value = coupon.type || '';
                            document.querySelector('#editAmount').value = coupon.discount || '';
                            document.querySelector('#editMinPurchase').value = coupon.minPurchase || '';
                            document.querySelector('#editStartDate').value = coupon.validFrom ? coupon.validFrom.split('T')[0] : '';
                            document.querySelector('#editEndDate').value = coupon.validUntil ? coupon.validUntil.split('T')[0] : '';
                            document.querySelector('#editUsageLimit').value = coupon.usageLimit || '';
                            document.querySelector('#editUsagePerCustomer').value = coupon.usagePerCustomer || '';
                            document.querySelector('#editDescription').value = coupon.description || '';
                            document.querySelector('#editApplicableTo').value = coupon.applicableTo || '';
                            document.querySelector('#editStatus').checked = coupon.status === 'active';
                            document.querySelector('#editCurrentUsage').textContent = coupon.currentUsage || 0;
                        })
                        .catch(error => {
                            console.error('Error fetching coupon data:', error);
                            alert('Failed to load coupon details. Please try again.');
                        });
                });
            });
        });


    </script>

    <script>

        document.querySelectorAll('.select-coupon').forEach(button => {
            button.addEventListener('click', async function () {
                const couponId = this.getAttribute('data-id');
                console.log(couponId)
                const response = await fetch(`/admin/coupon/details/${couponId}`, {
                    method: 'GET',
                    headers: {
                        'Content-Type': 'application/json'
                    }
                });

                if (response.ok) {
                    const coupon = await response.json();

                    document.querySelector('#coupon-code').innerText = coupon.code;
                    document.querySelector('#coupon-status').innerText = coupon.status;
                    document.querySelector('#coupon-discount').innerText = coupon.type === 'percentage' ? coupon.discount + '%' : 'â‚¹' + coupon.discount;
                    document.querySelector('#coupon-valid-from').innerText = new Date(coupon.validFrom).toLocaleDateString();
                    document.querySelector('#coupon-valid-until').innerText = new Date(coupon.validUntil).toLocaleDateString();
                    document.querySelector('#coupon-usage-limit').innerText = coupon.usageLimit;
                    document.querySelector('#coupon-used').innerText = coupon.used + ' times';
                    document.querySelector('#coupon-min-purchase').innerText = 'â‚¹' + coupon.minPurchase;

                } else {
                    alert('Failed to fetch coupon details');
                }
            });
        });
    </script>


